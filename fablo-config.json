{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.2.0/schema.json",
  "global": {
    "fabricVersion": "2.5.9",
    "tls": false,
    "peerDevMode": false,
    "tools": {
      "explorer": true
    }
  },
  "orgs": [
    {
      "organization": {
        "name": "PeternakanOrderer",
        "domain": "orderer.peternakan.com"
      },
      "orderers": [
        {
          "groupName": "peternakan-ordeerer-group",
          "type": "solo",
          "instances": 1
        }
      ]
    },
    {
      "organization": {
        "name": "Peternak",
        "domain": "peternak.peternakan.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "Admin",
        "domain": "admin.peternakan.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "DKPP",
        "domain": "dkpp.peternakan.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    }
  ],
  "channels": [
    {
      "name": "ternak-channel",
      "orgs": [
        {
          "name": "Peternak",
          "peers": [
            "peer0"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "ternak-chaincode",
      "version": "1.0.0",
      "lang": "node",
      "channel": "ternak-channel",
      "directory": "./chaincodes/ternak-chaincode"
    }
  ]
}