{
  "name": "fablo-test-network-peternak",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Peternak",
    "tlsEnable": false,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Peternak": {
      "mspid": "PeternakMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/peternak.peternakan.com/users/Admin@peternak.peternakan.com/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.peternak.peternakan.com"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/peternak.peternakan.com/users/Admin@peternak.peternakan.com/msp/signcerts/Admin@peternak.peternakan.com-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.peternak.peternakan.com": {
      "url": "grpc://peer0.peternak.peternakan.com:7041",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/peternak.peternakan.com/msp/tlscacerts/tlsca.peternak.peternakan.com-cert.pem"
      }
    }
  },
  "channels": {
    "ternak-channel": {
      "peers": {}
    }
  }
}
